{
  "name": "compare-anything",
  "sideEffects": false,
  "version": "0.3.4",
  "description": "Compares objects and arrays and tells you which props or values are duplicates, and which are only present once.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "repository": "https://github.com/mesqueeb/compare-anything",
  "author": "Luca Ban - Mesqueeb",
  "license": "MIT",
  "scripts": {
    "test": "ava",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "rollup": "rollup -c build/rollup.js",
    "build": "npm run lint && npm run rollup && npm run test",
    "release": "npm run build && np"
  },
  "dependencies": {
    "filter-anything": "^2.1.7",
    "is-what": "^3.11.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "ava": "^3.13.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-tree-shaking": "^1.8.0",
    "flatten-anything": "^2.0.3",
    "np": "^7.4.0",
    "prettier": "^2.2.1",
    "rollup": "^2.33.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "typescript",
    "javascript",
    "compare",
    "countif",
    "count-if",
    "duplicates",
    "compare-objects",
    "compare-arrays",
    "find-duplicates",
    "remove-duplicates"
  ],
  "bugs": {
    "url": "https://github.com/mesqueeb/compare-anything/issues"
  },
  "homepage": "https://github.com/mesqueeb/compare-anything#readme",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "tsconfig-paths/register",
      "ts-node/register"
    ]
  },
  "np": {
    "yarn": false,
    "branch": "production"
  }
}
