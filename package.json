{
  "name": "compare-anything",
  "sideEffects": false,
  "type": "module",
  "version": "1.0.1",
  "description": "Compares objects and arrays and tells you which props or values are duplicates, and which are only present once.",
  "module": "./dist/index.es.js",
  "main": "./dist/index.cjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12.13"
  },
  "repository": "https://github.com/mesqueeb/compare-anything",
  "author": "Luca Ban - Mesqueeb",
  "funding": "https://github.com/sponsors/mesqueeb",
  "license": "MIT",
  "scripts": {
    "test": "vitest run",
    "lint": "tsc --noEmit && eslint ./src --ext .ts",
    "build": "rollup -c ./scripts/build.js",
    "release": "npm run lint && del dist && npm run build && np"
  },
  "dependencies": {
    "filter-anything": "^2.2.1",
    "is-what": "^4.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "del-cli": "^4.0.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "flatten-anything": "^2.0.4",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "rollup": "^2.66.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^4.5.5",
    "vitest": "^0.2.1"
  },
  "keywords": [
    "typescript",
    "javascript",
    "compare",
    "countif",
    "count-if",
    "duplicates",
    "compare-objects",
    "compare-arrays",
    "find-duplicates",
    "remove-duplicates"
  ],
  "bugs": {
    "url": "https://github.com/mesqueeb/compare-anything/issues"
  },
  "homepage": "https://github.com/mesqueeb/compare-anything#readme",
  "np": {
    "yarn": false,
    "branch": "production"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "node_modules",
      "dist",
      "scripts",
      "test"
    ],
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "tree-shaking"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/ban-ts-ignore": "off",
      "tree-shaking/no-side-effects-in-initialization": "error",
      "@typescript-eslint/ban-ts-comment": "off"
    }
  }
}
